@using VideoGameCompendium.Models
@model List<Game>


@section Title{ 
    Browse Games
}

<div class="gameDisplay">
    @if (Model != null)
    {
        @foreach (Game game in Model)
        {
            <div class="GameCard">
                <partial name="_DisplayGamesHomePartial" model="game" />
            </div>
        }
    }
</div>
